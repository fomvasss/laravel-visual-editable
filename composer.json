{
    "name": "fomvasss/laravel-visual-editable",
    "description": "A package to visual edit your html sections, pages, blocks",
    "type": "composer-package",
    "keywords": ["cke", "editor", "visual", "blocks", "laravel"],
    "authors": [
        {
            "name": "Vasyl Fomin",
            "email": "fomvasss@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php" : ">=7.0.0",
        "illuminate/support": "5.6.*|5.7.*|5.8.*",
        "illuminate/database": "5.6.*|5.7.*|5.8.*",
        "illuminate/view": "5.6.*|5.7.*|5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Fomvasss\\LaravelVisualEditable\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fomvasss\\LaravelVisualEditable\\ServiceProvider"
            ],
            "aliases": {
                "ModelVisualEditor": "Fomvasss\\LaravelVisualEditable\\Facades\\ModelVisualEditor",
                "BlockVisualEditor": "Fomvasss\\LaravelVisualEditable\\Facades\\BlockVisualEditor"
            }
        }
    }
}
